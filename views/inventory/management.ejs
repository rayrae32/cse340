<div class="management-container">
  <h1>Inventory Management</h1>
  <% if (typeof messages !== 'undefined' && (messages.error ||
  messages.success)) { %>
  <div class="flash-message">
    <% if (messages.error) { %>
    <ul class="error">
      <% messages.error.forEach(msg => { %>
      <li><%= msg %></li>
      <% }) %>
    </ul>
    <% } %> <% if (messages.success) { %>
    <ul class="success">
      <% messages.success.forEach(msg => { %>
      <li><%= msg %></li>
      <% }) %>
    </ul>
    <% } %>
  </div>
  <% } %>
<p><a href="/inv/add-classification">Add New Classification</a> | <a href="/inv/add-inventory">Add New Vehicle</a></p>
<h2>Manage Inventory</h2>
<p>Select a classification from the list to see the items belonging to that classification.</p>
<%- classificationSelect %>
<table id="inventoryDisplay"></table>
<noscript>JavaScript must be enabled to use this page.</noscript>
<script src="../../js/inventory.js"></script>
</div>
